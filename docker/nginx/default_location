location /ws/ {
    proxy_pass         http://127.0.0.1:8001;
    proxy_http_version 1.1;
    proxy_set_header   Upgrade $http_upgrade;
    proxy_set_header   Connection "Upgrade";
    proxy_redirect off;
    proxy_read_timeout 86400;
}

location /static/ {
    alias /topblog/static/;
}

location /media/ {
    alias /topblog/media/;
}

location /vk/ {
    proxy_pass http://www.vk.com/;
}


