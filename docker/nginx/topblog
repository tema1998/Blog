

server {
    listen 80;
    server_name tema1998.ru;

    location / {
        # rewrite this section
        # where to redirect `/` requests
        return 301 https://$host$request_uri; # redirect all non https requests to https
    }

    location /.well-known/acme-challenge/ {
            root /var/www/certbot;
    }
}

